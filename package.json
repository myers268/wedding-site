{
	"name": "wedding-site",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"deploy": "alchemy deploy",
		"destroy": "alchemy destroy",
		"dev": "alchemy dev",
		"preview": "bun run build && vite preview",
		"typecheck": "react-router typegen && tsc -b",
		"db:generate": "drizzle-kit generate",
		"db:migrate-remote": "drizzle-kit migrate",
		"db:migrate-local": "wrangler d1 migrations apply registry-store --local",
		"studio:remote": "bunx drizzle-kit studio",
		"studio:local": "LOCAL_DB_PATH=$(find .alchemy/miniflare/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print0 | xargs -0 ls -t | head -1) drizzle-kit studio --config=drizzle.config.ts"
	},
	"imports": {
		"#app/*": "./app/*",
		"#db/*": "./app/db/*",
		"#services/*": "./app/services/*"
	},
	"dependencies": {
		"@epic-web/invariant": "^1.0.0",
		"@react-google-maps/api": "^2.20.7",
		"alchemy": "^0.60.0",
		"drizzle-orm": "^0.44.4",
		"drizzle-seed": "^0.3.1",
		"isbot": "^5.1.29",
		"react": "^19.1.1",
		"react-aria-components": "^1.12.1",
		"react-dom": "^19.1.1",
		"react-router": "^7.8.0",
		"tailwindcss-react-aria-components": "^2.0.1",
		"zod": "^3.25.67"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.11.2",
		"@cloudflare/workers-types": "^4.20250810.0",
		"@react-router/dev": "^7.8.0",
		"@tailwindcss/vite": "^4.1.11",
		"@types/google.maps": "^3.58.1",
		"@types/node": "^20",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"better-sqlite3": "^12.2.0",
		"drizzle-kit": "^0.31.4",
		"tailwindcss": "^4.1.11",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.9.2",
		"vite": "^7.1.1",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^4.28.1"
	}
}